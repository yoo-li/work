<?php/** * securitycheck:1表示不进行权限校验 */$Create = true;$Delete = true;$addlink = true;$ExportExcel = false;$MassSendApprove=false;  $actionmapping = array (     array('actionname' => 'ProductProperty','securitycheck' => '1','type'=>'ajax','location'=>'base','sequence'=>'2'),	array('actionname' => 'ProductLogo','securitycheck' => '1','type'=>'ajax','location'=>'panel'),     // array('actionname' => 'ParameterConfig','securitycheck' => '1','type'=>'ajax','location'=>'bottom'),   	array('actionname' => 'SimulateApply','securitycheck' => '1','type'=>'button','func'=>'check_SimulateApply'),	array('actionname' => 'UnSimulateApply','securitycheck' => '1','type'=>'button','func'=>'check_UnSimulateApply'),); if (!function_exists('check_SimulateApply')) {    function check_SimulateApply($module,$focus)    {        global $current_user,$supplierid,$supplierusertype;        if ( isset($supplierid) && $supplierid != ""  && $focus->column_fields['mall_productlibsstatus'] == "Saved")        {            if($focus->mode=='edit'){                if($focus->column_fields['approvalstatus'] != '2' )                {                    return '<a class="btn btn-default" data-toggle="dialog" data-icon="lock" data-mask="true" data-maxable="false" data-resizable="false" href="index.php?module='.$module.'&amp;action=SimulateApply&amp;record='.$focus->id.'" > 提交上线</a>';                }                else                {                    return '<a disabled class="btn btn-default" data-icon="lock" href="javascript:;" > 提交上线</a>';                }            }else{               return '<a disabled class="btn btn-default" data-icon="lock" href="javascript:;" > 提交上线</a>';            }        }		else{           return '<a disabled class="btn btn-default" data-icon="lock" href="javascript:;" > 提交上线</a>';        }    }}if (!function_exists('check_UnSimulateApply')) {    function check_UnSimulateApply($module,$focus)    {        global $current_user,$supplierid,$supplierusertype;        if ( isset($supplierid) && $supplierid != ""  && $focus->column_fields['approvalstatus'] == "2")        {            if($focus->mode=='edit'){                  return '<a class="btn btn-default" data-toggle="dialog" data-icon="unlock" data-mask="true" data-maxable="false" data-resizable="false" href="index.php?module='.$module.'&amp;action=UnSimulateApply&amp;record='.$focus->id.'" > 获得重新修改权限</a>';            }         }     }}?>